{
  "manifest_version": 1,
  "id": "com.cogitations.chatgpt-toolkit",
  "name": "ChatGPT Toolkit",
  "version": "1.0",
  "app_min_version": "2.12",
  "description": "Integrate ChatGPT AI assistant with your Joplin notes",
  "author": "Irv Shapiro",
  "license": "MIT",
  "homepage_url": "https://github.com/ishapiro/joplin-chatgpt-plugin",
